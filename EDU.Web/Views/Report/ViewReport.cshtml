@using MvcReportViewer
@{
    Layout = null;
}
@{
    var settings = new ControlSettings { ShowToolBar = true, ShowParameterPrompts = false };
    string url = Convert.ToString(ViewBag.url);
    string DateFrom = Convert.ToString(ViewBag.DateFrom);
    string DateTo = Convert.ToString(ViewBag.DateTo);
    string BranchID = Convert.ToString(ViewBag.BranchID);
    string DeclarationNo = Convert.ToString(ViewBag.DeclarationNo);
}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <title> View Report </title>
    <script src="@Url.Content("~/Scripts/jquery-2.2.4.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery-migrate-1.2.1.min.js")"></script>
    <script src="@Url.Content("~/Scripts/MvcReportViewer.js")"></script>
</head>
<body>
    
        @Html.MvcReportViewer(url,
        new
        {
            DeclarationNo = DeclarationNo,
            BranchID = BranchID
        },
        new
        {
            Height = 420,
            Width = "100%",
            style = "border: none;"
        }).ControlSettings(settings)
</body>
</html>


