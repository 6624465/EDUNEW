                    {
                        type: 'bar',
                        title: '',
                        fillStyles: ['#418CF0', '#FCB441', '#E0400A', '#056492', '#1A3B69'],
                        data: [
                        @foreach (var item in Model.GroupBy(x=>x.ProductName).ToList())
                        {
                            <text>
                                [
                                    '@item.FirstOrDefault().ProductName',
                                    @item.Sum(a=>a.AchievedRevenue)
                                ],
                            </text>
                        }
                        ]
                    }
                ]
            });

